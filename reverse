# include "monty.h"
/**
 * _rotl - rotate the list
 * @h: input node
 * @data: data;
 */
void _rotl(stack_t **h, int data)
{
        stack_t *tmp = *h, *pr = NULL;

        (void)data;

        if (head != NULL)
        {
                while ((*h)->next != NULL)
                {
                        *h = (*h)->next;
                        tmp->next = pr;
                        tmp->prev = *h;
                        pr = tmp;
                        tmp = *h;
                }
                (*h)->next = pr;
                (*h)->prev = NULL;
        }
}
